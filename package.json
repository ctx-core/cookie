{
	"name": "@ctx-core/cookie",
	"version": "10.4.11",
	"description": "ctx-core cookie",
	"keywords": [
		"ctx-core",
		"cookie"
	],
	"homepage": "https://github.com/ctx-core/cookie#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/cookie/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/cookie.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-cookie@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./cookie",
		"./cookie__get",
		"./cookie__is_present_",
		"./cookie__remove",
		"./cookie__set",
		"./cookie_key_a_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.16.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}